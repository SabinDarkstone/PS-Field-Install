<Window x:Class="PS_Field_Install.ProgressBar"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PS_Field_Install"
        mc:Ignorable="d"
        Title="Data Import Progress" Height="125.064" Width="354"
        ContentRendered="Window_ContentRendered" Topmost="True" ResizeMode="NoResize" ShowInTaskbar="False" WindowStartupLocation="CenterScreen">
    <Grid Margin="20,20,20,0" Height="29" VerticalAlignment="Top">
        <ProgressBar Minimum="0" Maximum="100" Name="pbStatus"/>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="0,45,0,-32" TextWrapping="Wrap" Text="Current Record:" VerticalAlignment="Top"/>
        <TextBlock x:Name="txtCurrRecordCICode" Margin="87.503,45,124.879,-31.96" TextWrapping="Wrap" VerticalAlignment="Top"/>
        <!--<TextBlock x:Name="txtCurrRecordDescription" HorizontalAlignment="Left" Margin="0,82,0,-69" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Width="306"/>-->
    </Grid>
</Window>