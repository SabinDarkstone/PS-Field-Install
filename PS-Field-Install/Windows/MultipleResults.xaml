<Window x:Class="PS_Field_Install.MultipleResults"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PS_Field_Install"
        mc:Ignorable="d"
        Title="MultipleResults" Height="316" Width="474" ResizeMode="NoResize" Topmost="True" WindowStartupLocation="CenterScreen" ShowInTaskbar="False" Icon="/PS-Field-Install;component/Assets/AcuityLogo.png" Unloaded="Window_Unloaded">
    <Window.Background>
        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
            <GradientStop Color="#FFD6D6D6" Offset="0"/>
            <GradientStop Color="White" Offset="1"/>
        </LinearGradientBrush>
    </Window.Background>
    <Grid Height="271" VerticalAlignment="Top">
        <ListBox x:Name="listResults" HorizontalAlignment="Left" Height="165" Margin="10,40,0,0" VerticalAlignment="Top" Width="446"/>
        <Grid Margin="135,220,153,29">
            <Button x:Name="btnConfirm" Content="Confirm" HorizontalAlignment="Right" Width="75" Click="btnConfirm_Click" Height="20" VerticalAlignment="Top" Margin="0,0,90,0"/>
            <Button x:Name="btnCancel" Content="Cancel" HorizontalAlignment="Right" Margin="0,0,10,0" VerticalAlignment="Top" Width="75"/>

        </Grid>

    </Grid>
</Window>
