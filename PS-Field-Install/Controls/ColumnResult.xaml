<UserControl x:Class="PS_Field_Install.Controls.ColumnResult"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PS_Field_Install.Controls"
             mc:Ignorable="d" d:DesignWidth="370" Width="375" Height="70">
    <Grid>
        <TextBlock x:Name="txtLabel" HorizontalAlignment="Left" TextWrapping="Wrap" Text="txtLabel" VerticalAlignment="Top" Width="215" FontSize="14.667" FontWeight="Bold" MouseLeftButtonUp="txtLabel_MouseLeftButtonUp"/>
        <TextBlock x:Name="txtResult" HorizontalAlignment="Left" Margin="0,20,-75,0" TextWrapping="Wrap" Text="txtResult" VerticalAlignment="Top" Height="32" Width="375"/>
    </Grid>
</UserControl>
